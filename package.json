{
  "name": "palavreco",
  "version": "2.0.0",
  "description": "The Discord bot that simulates Wordle in Portuguese",
  "main": "src/bot.ts",
  "scripts": {
    "dev": "tsnd --transpile-only src/bot.ts",
    "commands": "tsnd --transpile-only src/register.ts",
    "build": "tsc",
    "start": "node build/bot.js",
    "lint": "eslint src",
    "prettier": "prettier src --write"
  },
  "keywords": [
    "discord",
    "wordle",
    "bot"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/palavreco/Palavreco"
  },
  "bugs": {
    "url": "https://github.com/palavreco/Palavreco/issues"
  },
  "engines": {
    "node": ">=16.13.2"
  },
  "contributors": [
    "ADMVicli <admvicli@gmail.com>",
    "eduraio <eduardorodrigues011@icloud.com>",
    "vitorlops <milagre.vitor@gmail.com>"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@napi-rs/canvas": "^0.1.26",
    "dayjs": "^1.10.7",
    "discord-api-types": "^0.31.2",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "knex": "^2.2.0",
    "pg": "^8.7.3",
    "quickchart-js": "^3.0.2",
    "readline": "^1.3.0",
    "templite": "^1.2.0"
  },
  "devDependencies": {
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "chart.js": "^3.9.1",
    "eslint": "^8.11.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.4"
  }
