        "strict": true,
        "target": "ES6",
        "module": "CommonJS",
        "strictNullChecks": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "outDir": "./build",
    },
    "include": [
        "./src/**/*.ts",
        "*.ts",
    ],
}
